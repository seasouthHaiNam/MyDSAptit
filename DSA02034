#include<bits/stdc++.h>
using namespace std;
int n,a[20];
int check[20];
void in(){
    for(int k=1;k<=n;k++){
        cout<<a[k];
    }
    cout<<endl;
    }
void Try(int i){
    for(int j=1;j<=n;j++){
        if(check[j]==0){
        if(abs(a[i-1]-j)>1||i==1){
            a[i]=j;
            check[j]=1;
            if(i==n){
                in();
            }
            else{
                Try(i+1);
            }
            check[j]=0;
        }
    }
    }
}
int main(){
    int t;
    cin>>t;
    while(t--){
        cin>>n;
        Try(1);
    }

    }
