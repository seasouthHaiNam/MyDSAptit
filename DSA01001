#include<bits/stdc++.h>
using namespace std;
string next(string x){
    int i=x.size()-1;
    while(x[i]=='1'){
    x[i]='0';
    i--;
    }
    if(i>=0)x[i]='1';
    return x;
}
int main(){
    int t; cin>>t;
    getchar();
    while(t--){
            string x;
            getline(cin,x);
            x=next(x);
            cout<<x<<endl;
        }
    }

