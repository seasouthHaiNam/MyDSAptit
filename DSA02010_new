#include<bits/stdc++.h>
using namespace std;
int a[100],n,total;
vector<int>X;
int sum=0;
bool check=true;
void in(){
    cout<<"[";
    cout<<X[0];
    for(int i=1;i<X.size();i++){
        cout<<" "<<X[i];
    }
    cout<<"]";
    }
void Try(int i,int pos){
    for(int j=pos;j<=n;j++){
          sum+=a[j];
          X.push_back(a[j]);
          if(sum==total){
            in();
            check=false;
          }
          else if(sum<total){
            Try(i+1,j);
          }
          sum-=a[j];
          X.pop_back();
    }
    }

